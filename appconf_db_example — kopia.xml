<?xml version="1.0"?>
<ApplicationConfiguration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <SerializerConfig>
    <AssemblyCatalog>.</AssemblyCatalog>
    <AssemblyName>DbSerialize.dll</AssemblyName>
	<ConstructorArgs>
      <DBSerializer.ConnectionString>Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename={_DBPath};Integrated Security = True; Connect Timeout = 30</DBSerializer.ConnectionString>
    </ConstructorArgs>
  </SerializerConfig>
  <LoggerConfig>
    <AssemblyCatalog>.</AssemblyCatalog>
    <AssemblyName>DbLogging.dll</AssemblyName>
	<ConstructorArgs>
      <DbLogSaver.ConnectionString>Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename={_DBPath};Integrated Security = True; Connect Timeout = 30</DbLogSaver.ConnectionString>
    </ConstructorArgs>
  </LoggerConfig>
</ApplicationConfiguration>